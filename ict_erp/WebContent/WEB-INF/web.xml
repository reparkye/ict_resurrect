<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>ict_erp</display-name>
  <welcome-file-list>
    <welcome-file>/views/index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>DepartServlet</servlet-name>
    <servlet-class>com.ict.erp.controller.DepartServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DepartServlet</servlet-name>
    <url-pattern>/depart/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LvlServlet</servlet-name>
    <servlet-class>com.ict.erp.controller.LvlServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LvlServlet</servlet-name>
    <url-pattern>/lvl/*</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>GoServlet</servlet-name>
    <servlet-class>com.ict.erp.controller.GoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GoServlet</servlet-name>
    <url-pattern>/Go/*</url-pattern>
  </servlet-mapping>
</web-app>